{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "voice-client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*).(js|css|png|jpg|jpeg|gif|ico|json)",
      "dest": "/voice-client/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/voice-client/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
