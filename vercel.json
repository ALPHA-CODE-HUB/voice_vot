{
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "voice-client/package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "index.js" },
    { "src": "/(.*)", "dest": "voice-client/build/$1" }
  ],
  "env": {
    "OPENROUTER_API_KEY": "@openrouter_api_key"
  }
}
